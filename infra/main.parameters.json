{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "Environment": {
        "value": "${AZURE_ENVIRONMENT}"
      },
      "Instance": {
        "value": "${AZURE_INSTANCE}"
      },
      "Organization": {
        "value": "${AZURE_ORGANIZATION}"
      },
      "Project": {
        "value": "${AZURE_PROJECT}"
      },
      "Region": {
        "value": "${AZURE_REGION}"
      },
      "Workload": {
        "value": "${AZURE_WORKLOAD}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      }
    }
  }
  