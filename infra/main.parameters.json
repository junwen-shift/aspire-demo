{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "environment": {
        "value": "${AZURE_ENVIRONMENT}"
      },
      "instance": {
        "value": "${AZURE_INSTANCE}"
      },
      "organization": {
        "value": "${AZURE_ORGANIZATION}"
      },
      "project": {
        "value": "${AZURE_PROJECT}"
      },
      "region": {
        "value": "${AZURE_REGION}"
      },
      "workload": {
        "value": "${AZURE_WORKLOAD}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      }
    }
  }
  